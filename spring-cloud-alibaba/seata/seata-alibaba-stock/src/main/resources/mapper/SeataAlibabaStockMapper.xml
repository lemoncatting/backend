<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lemon.learn.mapper.SeataAlibabaStockMapper">

    <update id="reduce">
        UPDATE `stock`
        SET `count` = `count` - 1
        WHERE id = 1
    </update>
</mapper>
